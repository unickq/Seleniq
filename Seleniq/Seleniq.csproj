<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>
<!--    <TargetFrameworks>net45;net461;netstandard2.0</TargetFrameworks>-->
    <Authors>Nick Chursin</Authors>
    <PackageLicenseUrl>https://github.com/unickq/Seleniq/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/unickq/Seleniq</PackageProjectUrl>
    <RepositoryUrl>https://github.com/unickq/Seleniq</RepositoryUrl>
    <PackageTags>selenium</PackageTags>
    <Description>Seleniq - one more extensions project for selenium</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product></Product>
    <Company />
